{
    "mcpServers": {
        "context7": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp"
            ],
            "env": {}
        },
        "exa-search": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "exa-mcp-server"
            ],
            "env": {
                "EXA_API_KEY": "e779d92c-3c66-4209-85dd-ccf6a96fd89b"
            }
        },
        "fetch": {
            "type": "stdio",
            "command": "uvx",
            "args": [
                "mcp-server-fetch"
            ],
            "env": {}
        },
        "docker": {
            "type": "stdio",
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "mcp/fetch"
            ],
            "env": {}
        },
        "duckduckgo": {
            "type": "stdio",
            "command": "uvx",
            "args": [
                "duckduckgo-mcp-server"
            ],
            "env": {}
        },
        "sequentialthinking": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-sequential-thinking"
            ],
            "env": {}
        },
        "memory": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-memory"
            ],
            "env": {}
        }
    }
}